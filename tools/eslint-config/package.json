{
  "name": "@reactlith-template/eslint-config",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./eslint.config.js"
  },
  "scripts": {
    "clean": "bun exec \"rm -rf node_modules .turbo\"",
    "format": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --write",
    "format:check": "prettier . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --check",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@reactlith-template/prettier-config",
  "devDependencies": {
    "@eslint/compat": "~2.0.0",
    "@eslint/js": "~9.39.1",
    "@reactlith-template/typescript-config": "workspace:*",
    "@tanstack/eslint-plugin-query": "~5.91.2",
    "@tanstack/eslint-plugin-router": "~1.139.0",
    "@types/bun": "catalog:",
    "eslint": "~9.39.1",
    "eslint-plugin-react-compiler": "latest",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "~0.4.22",
    "typescript": "catalog:",
    "typescript-eslint": "~8.48.0"
  }
}
